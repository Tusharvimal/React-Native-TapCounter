{
  "expo": {
    "name": "Tap Counter",
    "slug": "tap-counter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f2f2f2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tushar.tapcounter",
      "buildNumber": "1.0.0" ,
      "supportsTablet": true
    },
    "android": {
      "package": "com.tushar.tapcounter" ,
      "versionCode": 1 ,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#C2185B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
